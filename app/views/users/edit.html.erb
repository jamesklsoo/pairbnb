<div class="row">
  <div class="col-md-8 col-md-offset-2 col-xs-8 col-xs-offset-2">
    <h1 class="text-center">
      Edit Your Profile
    </h1>
  </div>
</div>
<div class="row">
  <div class="col-md-8 col-md-offset-2 col-xs-8 col-xs-offset-2">
    <%= form_for @user do |form| %>
      <%= render partial: "/users/form", object: form %>
      <%= form.submit "Submit", class: "btn btn-default" %>
    <% end %>
  </div>
</div>